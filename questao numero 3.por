programa {
  funcao inicio() {
    
  //declaracao de variaveis
  inteiro a,b,soma,c,multplicacao,d

  //solicitacao de dados ao usuario
  escreva("Digite valor de A:")
  leia(a)
  escreva("Digite valor de B:")
  leia(b)

  //calculando
  se(a==b){soma=a+b
  c=soma}
  se(a!=b){multplicacao=a*b 
  d=multplicacao}


  //mostrando dados ao usuario
  escreva("\nC:" +c)
  escreva("\nD:" +d)
    
  }
}
